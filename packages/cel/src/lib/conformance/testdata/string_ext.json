          "expr":  "strings.quote(\"verbatim\") == \"\\\"verbatim\\\"\""
        },
        {
          "name":  "ends_with",
          "expr":  "strings.quote(\"ends with \\\\\") == \"\\\"ends with \\\\\\\\\\\"\""
        },
        {
          "name":  "starts_with",
          "expr":  "strings.quote(\"\\\\ starts with\") == \"\\\"\\\\\\\\ starts with\\\"\""
        },
        {
